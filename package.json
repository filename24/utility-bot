{
  "name": "utility-bot",
  "license": "MIT",
  "main": "build/index.js",
  "type": "module",
  "scripts": {
    "start": "yarn build && yarn start:node",
    "start:node": "node --enable-source-maps --experimental-specifier-resolution=node .",
    "build": "tsc && tsc-alias",
    "lint": "eslint . --ext .ts",
    "lint:fix": "prettier --write '**/*.ts' && eslint . --ext .ts --fix",
    "dev": "yarn generate && tsx watch src/index.ts ",
    "generate": "prisma generate",
    "update": "yarn upgrade-interactive"
  },
  "dependencies": {
    "@catppuccin/palette": "^0.2.0",
    "@prisma/client": "^5.5.2",
    "chalk": "5.3.0",
    "color-namer": "^1.4.0",
    "discord.js": "^14.13.0",
    "dokdo": "^1.0.1",
    "dotenv": "^16.3.1",
    "node-fetch": "3.3.2",
    "rotating-file-stream": "^3.1.1",
    "tslog": "^4.9.2",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/color-namer": "^1",
    "@types/node": "^18.18.8",
    "@types/node-fetch": "^2.6.8",
    "@types/uuid": "^9.0.6",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-require-extensions": "^0.1.3",
    "eslint-plugin-unused-imports": "^3.0.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "prisma": "^5.5.2",
    "tsc-alias": "^1.8.8",
    "tsx": "^3.14.0",
    "typescript": "^5.2.2"
  },
  "eslintIgnore": [
    "node_modules",
    "src/commands/",
    "example.*",
    "eslintrc.*"
  ],
  "engines": {
    "node": ">=16.9"
  },
  "packageManager": "yarn@3.2.2",
  "private": true,
  "resolutions": {
    "dokdo@^1.0.1": "patch:dokdo@npm:1.0.1#.yarn/patches/dokdo-npm-1.0.1-3b43b27044.patch"
  }
}
